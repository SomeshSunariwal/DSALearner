{
    "Problem Name": "Running Sum of 1D Array",
    "Description": "Given an array `nums`, return the running sum of `nums` where runningSum[i] = sum(nums[0..i]).",
    "Constraints": "1 <= nums.length <= 10^4\n-10^5 <= nums[i] <= 10^5",
    "Sample Input": [
        {
            "input": "[1,2,3,4]"
        }
    ],
    "Sample Output": [
        {
            "output": "[1,3,6,10]"
        }
    ],
    "Test Case Input": [
        {
            "input": "[1,2,3,4]"
        },
        {
            "input": "[1,1,1,1,1]"
        },
        {
            "input": "[3,1,2,10]"
        },
        {
            "input": "[-1,1,-1,1]"
        },
        {
            "input": "[5]"
        },
        {
            "input": "[]"
        }
    ],
    "Test Case Output": [
        {
            "output": "[1,3,6,10]"
        },
        {
            "output": "[1,2,3,4,5]"
        },
        {
            "output": "[3,4,6,16]"
        },
        {
            "output": "[-1,0,-1,0]"
        },
        {
            "output": "[5]"
        },
        {
            "output": "[]"
        }
    ]
}