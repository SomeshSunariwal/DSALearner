    "Constraints": "1 <= nums.length <= 10^5\n-10^4 <= nums[i] <= 10^4\nArray is sorted in non-decreasing order.",
    "Sample Input": [
        {
            "input": "[1,1,2]"
        }
    ],
    "Sample Output": [
        {
            "output": "2, nums = [1,2,_]"
        }
    ],
    "Test Case Input": [
        {
            "input": "[1,1,2]"
        },
        {
            "input": "[0,0,1,1,1,2,2,3,3,4]"
        },
        {
            "input": "[1]"
        },
        {
            "input": "[1,1,1,1]"
        },
        {
            "input": "[-1,0,0,0,3,3]"
        },
        {
            "input": "[2,2,2,3,3,4,4]"
        }
    ],
    "Test Case Output": [
        {
            "output": "2, nums = [1,2,_]"
        },
        {
            "output": "5, nums = [0,1,2,3,4,_]"
        },
        {
            "output": "1, nums = [1]"
        },
        {
            "output": "1, nums = [1]"
        },
        {
