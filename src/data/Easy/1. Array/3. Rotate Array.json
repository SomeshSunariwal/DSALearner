        }
    ],
    "Test Case Input": [
        {
            "input": "nums = [1,2,3,4,5,6,7], k = 3"
        },
        {
            "input": "nums = [-1,-100,3,99], k = 2"
        },
        {
            "input": "nums = [1,2], k = 3"
        },
        {
            "input": "nums = [1], k = 0"
        },
        {
            "input": "nums = [1,2,3], k = 0"
        },
        {
            "input": "nums = [1,2,3,4], k = 6"
        }
    ],
    "Test Case Output": [
        {
            "output": "[5,6,7,1,2,3,4]"
        },
        {
            "output": "[3,99,-1,-100]"
        },
        {
            "output": "[2,1]"
        },
        {
            "output": "[1]"
        },
        {
